    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: Normalize Forex DF Pipeline",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/src/dataflow/normalize_forex.py",
            "console": "integratedTerminal",
            "args": [
                "--config_bucket=lit-dark-markets",
                "--input_subscription_json=normalize_forex_config.json",
                "--otc_subscription=projects/lit-dark-markets/subscriptions/marketdata-forex-rt-json-sub",
                "--bq_output_table=lit-dark-markets:canon.test_trades",
                "--pubsub_output_topic=projects/lit-dark-markets/topics/marketdata-normalized-fx-trades-json",
                "--gcs_output_path=gs://lit-dark-markets-audit/",
                "--window_size=1.0",
                "--project=lit-dark-markets",
                "--runner=DirectRunner"
            ]
        },
        {
            "name": "Python: Replay Msg Utility",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/src/cloud_function/main.py",
            "console": "integratedTerminal",
            "args": [
                "./data/messages/krw_trd_json_export.csv",
                "TEST.SSCL.GCP.MD.RT.CMEG.JSON.v01000.TRD.XCME.KRW",
                "5"
            ]
        }
    ]
}